{
	"apps" : [{
		"name" : "liveWeb",  //名称
		"script": "./index.js", //程序入库
		"cwd": "./",           //根目录
		"env": {
			"IS_REMOTE":false,
			"NODE_ENV": "development"
		},
		"watch":[
			"bin",
			"common",
			"configs",
			"public",
			"routes",
			"views"
		],//需要监控的目录
		"error_file":"./logs/app-err.log",//错误输出日志
		"out_file":"./logs/app-out.log",  //日志
		"log_date_format":"YYYY-MM-DD HH:mm Z" //日期格式
	}]
}
